// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a,b,sum,multi,sub,div,mod;
    scanf("%d %d",&a,&b);
    sum=a+b;
    multi=a*b;
    sub=a-b;
    div=a/b;
    mod=a%b;
    printf("the sum of two numbers is :%d\n",sum);
    printf("the multiplication of two numbers is :%d\n",multi);
    printf("the difference of two numbers is :%d\n",sub);
    printf("the division of two numbers is :%d\n",div);
    printf("the mod of two numbers is :%d\n",mod);
    
    return 0;
}
